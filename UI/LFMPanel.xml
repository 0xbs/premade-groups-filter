<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="PremadeGroupsFilterLFMPanelTemplate" virtual="true" hidden="true">
        <Anchors>
            <Anchor point="TOPLEFT" y="-20"/>
            <Anchor point="BOTTOMRIGHT" y="30"/>
        </Anchors>
        <Frames>
            <Frame parentKey="Group">
                <Size x="400" y="250"/>
                <Anchors>
                    <Anchor point="TOPLEFT"/>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="Title" inherits="GameFontNormal" justifyH="LEFT">
                            <Size y="25"/>
                            <Anchors>
                                <Anchor point="TOP" y="-5"/>
                                <Anchor point="LEFT" x="10"/>
                                <Anchor point="RIGHT" x="-10"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Frames>
                    <Frame parentKey ="ItemLevel" inherits="PremadeGroupsFilterMinMaxTemplate">
                        <Anchors>
                            <Anchor point="TOP" relativeKey="$parent.Title" relativePoint="BOTTOM"/>
                            <Anchor point="LEFT" x="5"/>
                        </Anchors>
                    </Frame>
                    <Frame parentKey="MPRating" inherits="PremadeGroupsFilterMinMaxTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.ItemLevel" relativePoint="BOTTOMLEFT"/>
                        </Anchors>
                    </Frame>
                    <Frame parentKey="Tank" inherits="PremadeGroupsFilterBasicTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.MPRating" relativePoint="BOTTOMLEFT"/>
                        </Anchors>
                    </Frame>
                    <Frame parentKey="Healer" inherits="PremadeGroupsFilterBasicTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.Tank" relativePoint="BOTTOMLEFT"/>
                        </Anchors>
                    </Frame>
                    <Frame parentKey="DPS" inherits="PremadeGroupsFilterBasicTemplate">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativeKey="$parent.Healer" relativePoint="BOTTOMLEFT"/>
                        </Anchors>
                    </Frame>
                </Frames>
            </Frame>
            <Button parentKey="RefreshButton" inherits="MagicButtonTemplate">
                <Size x="135" y="22"/>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" x="-5" y="4"/>
                </Anchors>
            </Button>
        </Frames>
        <Scripts>
            <OnShow>
                self:OnShow()
            </OnShow>
            <OnHide>
                self:OnHide()
            </OnHide>
        </Scripts>
    </Frame>

</Ui>
